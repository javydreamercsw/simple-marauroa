<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-OSS Deploy</actionName>
        <displayName>OSS Deploy</displayName>
        <preAction>build-with-dependencies</preAction>
        <goals>
            <goal>clean</goal>
            <goal>install</goal>
            <goal>deploy:deploy</goal>
        </goals>
        <properties>
            <keystore>target/jwrestling.ks</keystore>
            <keystorepass>jwrestling4ever</keystorepass>
            <keystorealias>jwrestling</keystorealias>
        </properties>
        <activatedProfiles>
            <activatedProfile>release</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-OOSS Release Prepare</actionName>
        <displayName>OSS Release Prepare</displayName>
        <goals>
            <goal>release:clean</goal>
            <goal>release:prepare</goal>
        </goals>
        <properties>
            <pushChanges>false</pushChanges>
        </properties>
        <activatedProfiles>
            <activatedProfile>release</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-OOSS Release Rollback</actionName>
        <displayName>OSS Release Rollback</displayName>
        <goals>
            <goal>release:rollback</goal>
        </goals>
        <properties>
            <pushChanges>false</pushChanges>
        </properties>
        <activatedProfiles>
            <activatedProfile>release</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-OOSS Release Perform</actionName>
        <displayName>OSS Release Perform</displayName>
        <goals>
            <goal>release:perform</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>release</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-Deploy Files</actionName>
        <displayName>Deploy Files</displayName>
        <goals>
            <goal>deploy:deploy-file</goal>
        </goals>
        <properties>
            <repositoryId>ossrh</repositoryId>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
            <file></file>
        </properties>
        <activatedProfiles>
            <activatedProfile>release</activatedProfile>
        </activatedProfiles>
    </action>
</actions>
